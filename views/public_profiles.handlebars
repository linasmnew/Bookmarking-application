{{#section 'header'}}
  {{>header}}
{{/section}}

<div class="dashboard-wrapper">

  {{>public_profile_categories}}
<input id="username" type="hidden" value="{{username}}">
  <section class="view-bookmark-section grid-6">
    <h1 class="add-bookmark-h1">{{category}}</h1>

    <div id="view_bookmark_div">

      {{#each activity}}
        <p class="view_bookmark_url"><a href="http://{{url}}" target="_blank">{{url}}</a> </p>
        <p class="view_bookmark_dateAdded"> {{dateAdded}} </p>
        <div class="view_bookmark_tags"> {{#each this.tags}} <p class="tags"><a href="/user/bookmarks/tags/{{this}}">{{this}}</a></p> {{/each}}</div>
      {{/each}}

    </div>

    {{#section 'jquery'}}
      <script src="/js/load_more_for_visitors.js"></script>
    {{/section}}

  </section>

</div>
